{
  "extends": [
    "prettier",
    "prettier/react"
  ],
  "globals": {
    "fetch": false
  },
  "env": {
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {},
  "plugins": [
    "prettier",
    "react"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120
      }
    ]
  }
}